# Example environment configuration
# Copy this file to .env and fill in your own values

# Supabase configuration
# Client-side credentials for the Supabase project
VITE_SUPABASE_URL=http://localhost:54321
VITE_SUPABASE_ANON_KEY=dev-anon-key

# Optional service role key used by backend scripts
SUPABASE_SERVICE_ROLE_KEY=dev-service-role-key

# Backend API
VITE_BACKEND_URL=https://pam-backend.onrender.com
# WebSocket endpoint for PAM (override if needed)
VITE_PAM_WEBSOCKET_URL=wss://pam-backend.onrender.com/api/v1/pam/ws

# PAM Trip Planning Webhook (for Send to PAM functionality)
VITE_N8N_TRIP_WEBHOOK=https://treflip2025.app.n8n.cloud/webhook/4cd18979-6ee8-451e-b4e6-095c3d7ca31a

# Mapbox public access token (should start with 'pk.')
VITE_MAPBOX_TOKEN=pk.your_mapbox_token_here

# Default timeouts (in seconds)
HTTP_TIMEOUT=10
VITE_FETCH_TIMEOUT=10000

# Other environment variables can go here
