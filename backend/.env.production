# PAM Backend Production Environment
# Copy this file to .env and replace values with real production secrets

ENVIRONMENT=production
DEBUG=False
SECRET_KEY=your_production_secret_here
OPENAI_API_KEY=your_openai_api_key_here

# Database connection
DATABASE_URL=postgresql://username:password@db-host:5432/pam_backend
POSTGRES_DB=pam_backend
POSTGRES_USER=pam_user
POSTGRES_PASSWORD=your_db_password
POSTGRES_HOST=db-host
POSTGRES_PORT=5432

# Supabase configuration
SUPABASE_URL=https://your-prod.supabase.co
SUPABASE_KEY=your-prod-supabase-service-key
SUPABASE_ANON_KEY=your-prod-supabase-anon-key

# Redis / Cache
REDIS_URL=redis://:your_redis_password@redis-host:6379/0

# Allowed domains
CORS_ORIGINS=https://wheelsandwins.com,https://www.wheelsandwins.com

# Monitoring
SENTRY_DSN=your_sentry_dsn_here
