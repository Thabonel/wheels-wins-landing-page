{
  "name": "wheels-and-wins",
  "private": false,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "node scripts/build-debug.js && vite build",
    "build:netlify": "node scripts/netlify-build.js",
    "build:dev": "vite build --mode development",
    "build:staging": "vite build --mode staging",
    "build:production": "vite build --mode production",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "preview": "vite preview",
    "test": "vitest --run",
    "test:watch": "vitest",
    "test:coverage": "vitest --coverage --run",
    "test:ui": "vitest --ui",
    "test:integration": "vitest --config vitest.config.integration.ts --run",
    "test:integration:watch": "vitest --config vitest.config.integration.ts",
    "test:integration:coverage": "vitest --config vitest.config.integration.ts --coverage --run",
    "test:sentry": "node scripts/test-sentry.js",
    "e2e": "playwright test",
    "e2e:ui": "playwright test --ui",
    "e2e:ci": "playwright test --reporter=html",
    "test:crawl": "playwright test e2e/site-crawler.spec.ts --reporter=list",
    "test:pam:auto": "playwright test e2e/pam-automated-testing.spec.ts --reporter=list",
    "test:pam:auto:headed": "playwright test e2e/pam-automated-testing.spec.ts --headed",
    "test:pam:auto:debug": "playwright test e2e/pam-automated-testing.spec.ts --debug",
    "test:pam:pages": "playwright test e2e/pam-page-specific-tests.spec.ts --reporter=list",
    "test:pam:pages:headed": "playwright test e2e/pam-page-specific-tests.spec.ts --headed",
    "test:pam:pages:debug": "playwright test e2e/pam-page-specific-tests.spec.ts --debug",
    "type-check": "tsc --noEmit",
    "test:ci": "vitest --coverage --run --reporter=verbose",
    "quality:check": "npm run type-check && npm run lint && npm run test",
    "quality:check:full": "npm run type-check && npm run lint && npm run format:check && npm run test:coverage && npm run e2e:ci",
    "quality:fix": "npm run lint:fix && npm run format",
    "security:audit": "npm audit --audit-level high",
    "security:audit:json": "npm audit --audit-level high --json",
    "security:fix": "npm audit fix",
    "security:fix:force": "npm audit fix --force",
    "security:check:full": "npm run security:audit && npm run type-check",
    "ci:full": "npm run quality:check && npm run build && npm run e2e",
    "test:staging": "node scripts/test-staging.js",
    "seo:sitemap": "node scripts/generate-sitemap.js",
    "seo:audit": "node scripts/seo-audit.js",
    "seo:prerender": "node scripts/prerender.js",
    "seo:lighthouse": "lhci autorun",
    "optimize:images": "node scripts/optimize-images.js",
    "postbuild": "npm run seo:sitemap",
    "prepare": "husky",
    "postinstall": "husky && node scripts/ensure-native-deps.js",
    "ai-agents": "node scripts/ai-agents.js",
    "ai:status": "npm run ai-agents status",
    "ai:security": "npm run ai-agents security",
    "ai:tests": "npm run ai-agents tests",
    "test:validate-integration": "node scripts/validate-integration-tests.js",
    "pam:validate-removal": "node scripts/validate-pam-removal.js",
    "pam:validate-context": "bash scripts/validate-pam-context.sh",
    "check-main": "bash scripts/check-main.sh",
    "ai:docs": "npm run ai-agents docs",
    "ai:pam": "npm run ai-agents pam",
    "ai:all": "npm run ai-agents all",
    "agents:init": "./scripts/init-agents.sh",
    "agents:check": "./scripts/init-agents.sh",
    "translate": "tsx scripts/translate.ts",
    "translate:missing": "tsx scripts/translate.ts --missing-only",
    "translate:lang": "tsx scripts/translate.ts --lang"
